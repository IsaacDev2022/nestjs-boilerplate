{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "webpack": true
  },
  "projects": {
    "exceptions": {
      "type": "library",
      "root": "libs/exceptions",
      "entryFile": "index",
      "sourceRoot": "libs/exceptions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/exceptions/tsconfig.lib.json"
      }
    },
    "swagger-decorators": {
      "type": "library",
      "root": "libs/swagger-decorators",
      "entryFile": "index",
      "sourceRoot": "libs/swagger-decorators/src",
      "compilerOptions": {
        "tsConfigPath": "libs/swagger-decorators/tsconfig.lib.json"
      }
    }
  }
}